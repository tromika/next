{
    "name": "Devcontainer",
    "image": "ghcr.io/codesandbox/devcontainers/typescript-node:latest",
    "postCreateCommand": "yarn install && yarn start",
    "forwardPorts": [3000],
    "portsAttributes": {
        "8888": {
          "label": "Nextjs",
          "requireLocalPort": true,
          "onAutoForward": "ignore"
        }
      }
}